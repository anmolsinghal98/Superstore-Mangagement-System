<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="496.0" prefWidth="600.0" styleClass="background" stylesheets="@styles.css" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.surfStores">
   <children>
      <Button layoutX="507.0" layoutY="21.0" mnemonicParsing="false" onAction="#backPressed" prefHeight="49.0" prefWidth="71.0" styleClass="contact-us" stylesheets="@styles.css" text="Back" />
      <ImageView fitHeight="150.0" fitWidth="200.0" layoutY="-5.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../assets/Logo.png" />
         </image>
      </ImageView>
      <TableView fx:id="table" layoutX="21.0" layoutY="135.0" prefHeight="306.0" prefWidth="566.0">
        <columns>
          <TableColumn fx:id="id" prefWidth="75.0" text="Store ID" />
          <TableColumn fx:id="name" prefWidth="230.0" text="Name of The Store" />
            <TableColumn fx:id="loc" prefWidth="260.0" text="Store Location" />
        </columns>
      </TableView>
      <Button layoutX="255.0" layoutY="450.0" mnemonicParsing="false" onAction="#HandleContinue" styleClass="choose-role-button" stylesheets="@styles.css" text="Continue" />
   </children>
</AnchorPane>
